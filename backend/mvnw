#!/bin/sh
# Apache Maven Wrapper startup script
MVNW_REPOURL="https://repo.maven.apache.org/maven2"
WRAPPER_URL="${MVNW_REPOURL}/io/takari/maven-wrapper/0.5.6/maven-wrapper-0.5.6.jar"
BASE_DIR=$(dirname "$0")
MAVEN_WRAPPER_JAR="$BASE_DIR/.mvn/wrapper/maven-wrapper.jar"
if [ ! -f "$MAVEN_WRAPPER_JAR" ]; then
  mkdir -p "$BASE_DIR/.mvn/wrapper"
  curl -fsSL "$WRAPPER_URL" -o "$MAVEN_WRAPPER_JAR"
  echo "distributionUrl=https://repo.maven.apache.org/maven2/org/apache/maven/apache-maven/3.9.9/apache-maven-3.9.9-bin.zip" > "$BASE_DIR/.mvn/wrapper/maven-wrapper.properties"
fi
JAVA_CMD=java
exec "$JAVA_CMD" -cp "$MAVEN_WRAPPER_JAR" org.apache.maven.wrapper.MavenWrapperMain "$@"
